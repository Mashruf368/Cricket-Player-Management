<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="850.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.SecondController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <AnchorPane prefHeight="523.0" prefWidth="810.0">
        <children>
            <Button layoutX="224.0" layoutY="50.0" mnemonicParsing="false" onAction="#onSearchPlayerClick" prefHeight="74.0" prefWidth="339.0" text="SEARCH PLAYERS">
                <font>
                    <Font size="24.0" />
                </font>
            </Button>
            <Button layoutX="224.0" layoutY="160.0" mnemonicParsing="false" onAction="#onSearchClubClick" prefHeight="74.0" prefWidth="339.0" text="SEARCH CLUBS">
                <font>
                    <Font size="24.0" />
                </font>
            </Button>
            <Button layoutX="224.0" layoutY="277.0" mnemonicParsing="false" onAction="#onaddplayerclick" prefHeight="74.0" prefWidth="339.0" text="ADD PLAYER">
                <font>
                    <Font size="18.0" />
                </font>
            </Button>
            <Button layoutX="224.0" layoutY="380.0" mnemonicParsing="false" onAction="#onExitClick" prefHeight="74.0" prefWidth="339.0" text="EXIT ">
                <font>
                    <Font size="18.0" />
                </font>
            </Button>
        </children>
    </AnchorPane>

    <Label fx:id="secondtext" />
</VBox>
